<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Dispatch.
    -->
    <changeSet id="20201027211627-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="transaction_id"
                                 baseTableName="dispatch"
                                 constraintName="fk_dispatch_transaction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="stock_transaction"/>

        <addForeignKeyConstraint baseColumnNames="transporter_id"
                                 baseTableName="dispatch"
                                 constraintName="fk_dispatch_transporter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transport_details"/>

        <addForeignKeyConstraint baseColumnNames="transporter2_id"
                                 baseTableName="dispatch"
                                 constraintName="fk_dispatch_transporter2_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transport_details"/>

    </changeSet>
</databaseChangeLog>
